{
    "name": "ilgazil/downloader",
    "description": "Host downloader",
    "keywords": ["host", "downloader", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ilgazil/downloader/issues",
        "source": "https://github.com/ilgazil/downloader"
    },
    "authors": [
        {
            "name": "Thomas Vivien",
            "email": "thomas+github@vivien.cloud"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ilgazil/lib-download"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "anlutro/curl": "^1.5",
        "ilgazil/lib-download": "dev-main",
        "illuminate/database": "^8.40",
        "laravel-zero/framework": "^8.8",
        "paquettg/php-html-parser": "^3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["host-downloader"]
}
